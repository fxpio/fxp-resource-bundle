{
    "name": "sonatra/resource-bundle",
    "type": "symfony-bundle",
    "description": "Resource management layer for doctrine",
    "keywords": ["resource", "doctrine", "batch", "manager", "validation"],
    "homepage": "https://github.com/sonatra/SonatraResourceBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "FranÃ§ois Pluchino",
            "email": "francois.pluchino@sonatra.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "symfony/framework-bundle": "~3.0",
        "sonatra/default-value-bundle": "1.0.*@dev",
        "sonatra/resource": "1.0.*@dev",
        "sonatra/default-value": "1.0.*@dev"
    },
    "autoload": {
        "psr-4": { "Sonatra\\Bundle\\ResourceBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
