    "keywords": ["resource", "doctrine", "batch", "manager", "validation"],
    "homepage": "https://github.com/sonatra/SonatraResourceBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "FranÃ§ois Pluchino",
            "email": "francois.pluchino@sonatra.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": "~2.5",
        "symfony/event-dispatcher": "~2.5",
        "symfony/validator": "~2.5",
        "doctrine/doctrine-bundle": "~1.2",
        "sonatra/default-value-bundle": "1.0.*@dev"
    },
    "require-dev": {
        "doctrine/orm": "~2.4",
        "doctrine/doctrine-fixtures-bundle": "~2.2",
        "liip/functional-test-bundle":"~1.1"
